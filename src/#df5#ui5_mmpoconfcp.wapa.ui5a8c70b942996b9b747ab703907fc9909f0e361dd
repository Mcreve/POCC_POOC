sap.ui.define([], function() {                                                                                                                                                                                                                                 
	"use strict";                                                                                                                                                                                                                                                 
	return {                                                                                                                                                                                                                                                      
		formatSAPDate: function (dValue) {                                                                                                                                                                                                                           
			if(dValue){                                                                                                                                                                                                                                                 
			let dateFormat = sap.ui.core.format.DateFormat.getDateTimeInstance({                                                                                                                                                                                        
				pattern: "ddMMyyyy"                                                                                                                                                                                                                                        
			});                                                                                                                                                                                                                                                         
                                                                                                                                                                                                                                                               
			return dateFormat.format(dValue);                                                                                                                                                                                                                           
			}                                                                                                                                                                                                                                                           
			else{                                                                                                                                                                                                                                                       
				return "";                                                                                                                                                                                                                                                 
			}                                                                                                                                                                                                                                                           
		},                                                                                                                                                                                                                                                           
		                                                                                                                                                                                                                                                             
		formatDate: function (sValue) {                                                                                                                                                                                                                              
			if (sValue) {                                                                                                                                                                                                                                               
				if (sValue !== '00000000') {                                                                                                                                                                                                                               
					let sDateFormat = sap.ui.getCore().getConfiguration().getFormatSettings().getDatePattern("medium");                                                                                                                                                       
					let sSeperator;                                                                                                                                                                                                                                           
					if(sDateFormat.includes(".",0)){                                                                                                                                                                                                                          
						sSeperator = ".";                                                                                                                                                                                                                                        
					}                                                                                                                                                                                                                                                         
					else if(sDateFormat.includes("/",0)){                                                                                                                                                                                                                     
						sSeperator = "/";                                                                                                                                                                                                                                        
					}                                                                                                                                                                                                                                                         
					                                                                                                                                                                                                                                                          
					let aParts = sValue.split(sSeperator);                                                                                                                                                                                                                    
					                                                                                                                                                                                                                                                          
					let iDay = parseInt(aParts[0],0);                                                                                                                                                                                                                         
					let iMonth = parseInt(aParts[1],0) - 1;                                                                                                                                                                                                                   
					let iYear = parseInt(aParts[2],0);                                                                                                                                                                                                                        
					 return new Date(iYear, iMonth , iDay, 8, 0 , 0);                                                                                                                                                                                                         
				} else {                                                                                                                                                                                                                                                   
					return "";                                                                                                                                                                                                                                                
				}                                                                                                                                                                                                                                                          
			} else {                                                                                                                                                                                                                                                    
				return "";                                                                                                                                                                                                                                                 
			}                                                                                                                                                                                                                                                           
		},                                                                                                                                                                                                                                                           
		                                                                                                                                                                                                                                                             
		formatLineStatusIcon: function (sStatus) {                                                                                                                                                                                                                   
			switch (sStatus) {                                                                                                                                                                                                                                          
			case 0:                                                                                                                                                                                                                                                     
				return "sap-icon://status-inactive";                                                                                                                                                                                                                       
			case 3:                                                                                                                                                                                                                                                     
				return "sap-icon://status-positive";                                                                                                                                                                                                                       
			default:                                                                                                                                                                                                                                                    
				return "sap-icon://status-negative";                                                                                                                                                                                                                       
			}                                                                                                                                                                                                                                                           
		},                                                                                                                                                                                                                                                           
                                                                                                                                                                                                                                                               
		formatLineStatusState: function (sStatus) {                                                                                                                                                                                                                  
			switch (sStatus) {                                                                                                                                                                                                                                          
			case 0:                                                                                                                                                                                                                                                     
				return "None";                                                                                                                                                                                                                                             
			case 3:                                                                                                                                                                                                                                                     
				return "Success";                                                                                                                                                                                                                                          
			default:                                                                                                                                                                                                                                                    
				return "Error";                                                                                                                                                                                                                                            
			}                                                                                                                                                                                                                                                           
		}                                                                                                                                                                                                                                                            
		                                                                                                                                                                                                                                                             
	};                                                                                                                                                                                                                                                            
});                                                                                                                                                                                                                                                            