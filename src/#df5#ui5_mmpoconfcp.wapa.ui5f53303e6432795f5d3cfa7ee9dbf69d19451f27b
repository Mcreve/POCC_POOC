sap.ui.define([                                                                                                                                                                                                                                                
	"sap/ui/model/Filter",                                                                                                                                                                                                                                        
	"sap/m/MessageBox",                                                                                                                                                                                                                                           
	"sap/m/MessageToast"                                                                                                                                                                                                                                          
], function (Filter, MessageBox, MessageToast) {                                                                                                                                                                                                               
	"use strict";                                                                                                                                                                                                                                                 
                                                                                                                                                                                                                                                               
	return {                                                                                                                                                                                                                                                      
		/*                                                                                                                                                                                                                                                           
		 * Method that creates a dialog                                                                                                                                                                                                                              
		 * @public                                                                                                                                                                                                                                                   
		 * @param {that} the view                                                                                                                                                                                                                                    
		 * @param {oContent} the text or inputs that needs to be displayed                                                                                                                                                                                           
		 * @param {sTitle} the title                                                                                                                                                                                                                                 
		 * @param {oButton} the button with eventhandlers                                                                                                                                                                                                            
		 * @return {oDialog} returns the created dialog	&nbsp;                                                                                                                                                                                                       
		 */                                                                                                                                                                                                                                                          
		genericDialog: function (that, sTitle, oContent, oButton) {                                                                                                                                                                                                  
			let oDialog = new sap.m.Dialog({                                                                                                                                                                                                                            
				id: "genericDialog",                                                                                                                                                                                                                                       
				title: sTitle,                                                                                                                                                                                                                                             
				type: "Message",                                                                                                                                                                                                                                           
				content: oContent,                                                                                                                                                                                                                                         
				beginButton: oButton,                                                                                                                                                                                                                                      
				endButton: new sap.m.Button({                                                                                                                                                                                                                              
					text: that.getView().getModel("i18n").getResourceBundle().getText("Generic.Cancel"),                                                                                                                                                                      
					press: function () {                                                                                                                                                                                                                                      
						oDialog.close();                                                                                                                                                                                                                                         
					}                                                                                                                                                                                                                                                         
				}),                                                                                                                                                                                                                                                        
				afterClose: function () {                                                                                                                                                                                                                                  
					oDialog.destroy();                                                                                                                                                                                                                                        
				}                                                                                                                                                                                                                                                          
			});                                                                                                                                                                                                                                                         
			return oDialog;                                                                                                                                                                                                                                             
		},                                                                                                                                                                                                                                                           
                                                                                                                                                                                                                                                               
		/*                                                                                                                                                                                                                                                           
		 * Method that creates a button with an event for a dialog                                                                                                                                                                                                   
		 * @public                                                                                                                                                                                                                                                   
		 * @param {that} the view                                                                                                                                                                                                                                    
		 * @param {oTextResource} the text resource (i18n)                                                                                                                                                                                                           
		 * @param {oElements} contains some elements to specify the model, ...                                                                                                                                                                                       
		 * @param {fExecute} the function that gets executed upon button press                                                                                                                                                                                       
		 * @return {oButton} the button that was created&nbsp;                                                                                                                                                                                                       
		 */                                                                                                                                                                                                                                                          
		genericDialogButtonEvent: function (that, oTextResource, oElements, fExecute) {                                                                                                                                                                              
			let oButton = new sap.m.Button({                                                                                                                                                                                                                            
				text: oTextResource.getText("Generic.Submit"),                                                                                                                                                                                                             
				press: function () {                                                                                                                                                                                                                                       
					let sInput = sap.ui.getCore().byId(oElements.sInputId).getValue();                                                                                                                                                                                        
					fExecute(that, oElements.oPayload, sInput);                                                                                                                                                                                                               
				}                                                                                                                                                                                                                                                          
			});                                                                                                                                                                                                                                                         
			return oButton;                                                                                                                                                                                                                                             
		},                                                                                                                                                                                                                                                           
                                                                                                                                                                                                                                                               
		/*                                                                                                                                                                                                                                                           
		 * Method that creates a button for a dialog                                                                                                                                                                                                                 
		 * @public                                                                                                                                                                                                                                                   
		 * @param {that} the view                                                                                                                                                                                                                                    
		 * @param {oTextResource} the text resource (i18n)                                                                                                                                                                                                           
		 * @param {oElements} contains some elements to specify the model, ...                                                                                                                                                                                       
		 * @param {fExecute} the function that gets executed upon button press                                                                                                                                                                                       
		 * @return {oButton} the button that was created&nbsp;                                                                                                                                                                                                       
		 */                                                                                                                                                                                                                                                          
		genericDialogButton: function (that, oTextResource, oElements, fExecute) {                                                                                                                                                                                   
			let oButton = new sap.m.Button({                                                                                                                                                                                                                            
				text: oTextResource, //; .getText("Generic.Submit"),                                                                                                                                                                                                       
				press: function (oEvent) {                                                                                                                                                                                                                                 
					fExecute(that, oElements);                                                                                                                                                                                                                                
					oEvent.getSource().getParent().close();                                                                                                                                                                                                                   
				}                                                                                                                                                                                                                                                          
			});                                                                                                                                                                                                                                                         
			return oButton;                                                                                                                                                                                                                                             
		},                                                                                                                                                                                                                                                           
                                                                                                                                                                                                                                                               
		/*                                                                                                                                                                                                                                                           
		 * Method that creates the content (including an input field) for a dialog                                                                                                                                                                                   
		 * @public                                                                                                                                                                                                                                                   
		 * @param {that} the view                                                                                                                                                                                                                                    
		 * @param {oTextResource} the text resource (i18n)                                                                                                                                                                                                           
		 * @param {oElements} contains some elements to specify the model, ...                                                                                                                                                                                       
		 * @return {oContent} the content that was created&nbsp;                                                                                                                                                                                                     
		 */                                                                                                                                                                                                                                                          
		genericDialogContentInput: function (that, oTextResource, oElements) {                                                                                                                                                                                       
			let oContent = [                                                                                                                                                                                                                                            
				new sap.m.Text({                                                                                                                                                                                                                                           
					text: oTextResource.getText(oElements.sText1) + " " + oElements.oPayload.length + " " + oTextResource.getText(oElements.sText2)                                                                                                                           
				}),                                                                                                                                                                                                                                                        
				new sap.m.Input({                                                                                                                                                                                                                                          
					id: oElements.sInputId,                                                                                                                                                                                                                                   
					required: true                                                                                                                                                                                                                                            
				})                                                                                                                                                                                                                                                         
			];                                                                                                                                                                                                                                                          
			return oContent;                                                                                                                                                                                                                                            
		},                                                                                                                                                                                                                                                           
                                                                                                                                                                                                                                                               
		/*                                                                                                                                                                                                                                                           
		 * Method that creates the content for a dialog                                                                                                                                                                                                              
		 * @public                                                                                                                                                                                                                                                   
		 * @param {that} the view                                                                                                                                                                                                                                    
		 * @param {oTextResource} the text resource (i18n)                                                                                                                                                                                                           
		 * @param {oElements} contains some elements to specify the model, ...                                                                                                                                                                                       
		 * @return {oContent} the content that was created&nbsp;                                                                                                                                                                                                     
		 */                                                                                                                                                                                                                                                          
		genericDialogContentText: function (that, oTextResource, oElements) {                                                                                                                                                                                        
			let oContent = new sap.m.Text({                                                                                                                                                                                                                             
				text: oTextResource.getText(oElements.sText1) + oElements.oPayload.length + " " + oTextResource.getText(oElements.sText2)                                                                                                                                  
			});                                                                                                                                                                                                                                                         
			return oContent;                                                                                                                                                                                                                                            
		},                                                                                                                                                                                                                                                           
                                                                                                                                                                                                                                                               
		/*                                                                                                                                                                                                                                                           
		 * Method that creates the content (including an input field) for a dialog                                                                                                                                                                                   
		 * @public                                                                                                                                                                                                                                                   
		 * @param {that} the view                                                                                                                                                                                                                                    
		 * @param {oTextResource} the text resource (i18n)                                                                                                                                                                                                           
		 * @param {oElements} contains some elements to specify the model, ...                                                                                                                                                                                       
		 * @return {oContent} the content that was created&nbsp;                                                                                                                                                                                                     
		 */                                                                                                                                                                                                                                                          
		genericDialogContentName: function (that, oTextResource, oElements) {                                                                                                                                                                                        
			let oContent = new sap.m.Text({                                                                                                                                                                                                                             
				text: oTextResource.getText(oElements.sText1) + oElements.FULLNAME                                                                                                                                                                                         
			});                                                                                                                                                                                                                                                         
			return oContent;                                                                                                                                                                                                                                            
		},                                                                                                                                                                                                                                                           
                                                                                                                                                                                                                                                               
		/*                                                                                                                                                                                                                                                           
		 * Method that creates a dialog                                                                                                                                                                                                                              
		 * @public                                                                                                                                                                                                                                                   
		 * @param {sTitle} type string, title of the dialog                                                                                                                                                                                                          
		 * @param {sText} the text that needs to be displayed = content, type string                                                                                                                                                                                 
		 * @param {sButtonTextB} type string                                                                                                                                                                                                                         
		 * @param {sButtonTextE} type string                                                                                                                                                                                                                         
		 */                                                                                                                                                                                                                                                          
		showInformationDialog: function (sTitle, sText, sButtonTextB, sState , oButton) {                                                                                                                                                                            
			let oDialog = new sap.m.Dialog({                                                                                                                                                                                                                            
				title: sTitle,                                                                                                                                                                                                                                             
				type: "Message",                                                                                                                                                                                                                                           
				state: sState,                                                                                                                                                                                                                                             
				content: new sap.m.Text({                                                                                                                                                                                                                                  
					text: sText                                                                                                                                                                                                                                               
				}),                                                                                                                                                                                                                                                        
				beginButton: oButton,                                                                                                                                                                                                                                      
				afterClose: function () {                                                                                                                                                                                                                                  
					oDialog.destroy();                                                                                                                                                                                                                                        
				}                                                                                                                                                                                                                                                          
			});                                                                                                                                                                                                                                                         
			oDialog.open();                                                                                                                                                                                                                                             
		},                                                                                                                                                                                                                                                           
			showDialog: function (sTitle, sText, sButtonTextB, sState) {                                                                                                                                                                                                
			let oDialog = new sap.m.Dialog({                                                                                                                                                                                                                            
				title: sTitle,                                                                                                                                                                                                                                             
				type: "Message",                                                                                                                                                                                                                                           
				state: sState,                                                                                                                                                                                                                                             
				content: new sap.m.Text({                                                                                                                                                                                                                                  
					text: sText                                                                                                                                                                                                                                               
				}),                                                                                                                                                                                                                                                        
				beginButton: new sap.m.Button({                                                                                                                                                                                                                            
				text: sButtonTextB,                                                                                                                                                                                                                                        
				press: function () {                                                                                                                                                                                                                                       
					oDialog.close();                                                                                                                                                                                                                                          
				}                                                                                                                                                                                                                                                          
			}),                                                                                                                                                                                                                                                         
				afterClose: function () {                                                                                                                                                                                                                                  
					oDialog.destroy();                                                                                                                                                                                                                                        
				}                                                                                                                                                                                                                                                          
			});                                                                                                                                                                                                                                                         
			oDialog.open();                                                                                                                                                                                                                                             
		},                                                                                                                                                                                                                                                           
                                                                                                                                                                                                                                                               
		/*                                                                                                                                                                                                                                                           
		 * Method that creates an error message box                                                                                                                                                                                                                  
		 * @public                                                                                                                                                                                                                                                   
		 * @param {sErrorMessage} the error message text type String                                                                                                                                                                                                 
		 */                                                                                                                                                                                                                                                          
		showMessageBox: function (sErrorMessage) {                                                                                                                                                                                                                   
			MessageBox.error("Generic error", {                                                                                                                                                                                                                         
				details: sErrorMessage                                                                                                                                                                                                                                     
			});                                                                                                                                                                                                                                                         
		},                                                                                                                                                                                                                                                           
                                                                                                                                                                                                                                                               
		/*                                                                                                                                                                                                                                                           
		 * Method that creates an message toast                                                                                                                                                                                                                      
		 * @public                                                                                                                                                                                                                                                   
		 * @param {sErrorMessage} the error message text type String                                                                                                                                                                                                 
		 */                                                                                                                                                                                                                                                          
		showMessageToast: function (sMessage) {                                                                                                                                                                                                                      
			MessageToast.show(sMessage);                                                                                                                                                                                                                                
		}                                                                                                                                                                                                                                                            
	};                                                                                                                                                                                                                                                            
});                                                                                                                                                                                                                                                            